-- DROP TABLE Digicolor_FastnessTentativeRatingProducts
CREATE TABLE Digicolor_FastnessTentativeRatingProducts
(
	Id INT IDENTITY(1,1) PRIMARY KEY,
	ProductName VARCHAR(8000),
	ProductCode VARCHAR(500),
	ProductRange NVARCHAR(50)
)

-- DROP TABLE Digicolor_FastnessTechnicalData 
CREATE TABLE Digicolor_FastnessTechnicalData (
    ResultId INT PRIMARY KEY IDENTITY(1,1),
    ProductId INT FOREIGN KEY REFERENCES   Digicolor_FastnessTentativeRatingProducts(Id),
    Depth NVARCHAR(10),
	FastnessName  VARCHAR(1000),
    TestMethod NVARCHAR(100),
	Staning NVARCHAR(10),    
	Value NVARCHAR(50)
);


