SELECT prod.ProductCode,prod.ProductName,Depth,FastnessName,TestMethod,Staning,Value  FROM Digicolor_FastnessTentativeRatingProducts AS prod WITH(NOLOCK)
INNER JOIN Digicolor_FastnessTechnicalData AS mainData WITH(NOLOCK) ON prod.Id = mainData.ProductId
